#include <stdio.h>
 
int main() {
 
   double A, B, C, X1, X2;
    scanf("%lf%lf%lf", &A,&B,&C);
    X1=(-B+(sqrt(B*B-4*A*C)))/(2*A);
    X2=(-B-(sqrt(B*B-4*A*C)))/(2*A);
    if(A==0 || (B*B-4*A*C)<=0)
    {
        printf("Impossivel calcular\n");
    }
    else
    {
        printf("R1 = %.5lf\nR2 = %.5lf\n",X1,X2);
    }
 
    return 0;
}
