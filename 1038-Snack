#include<stdio.h>
int main()
{
    int x,y;
    double n;
    scanf("%d %d", &x,&y);
    if(x==1)
        n=4.00*y;
    else if(x==2)
        n=4.50*y;
    else if(x==3)
        n=5.00*y;
    else if(x==4)
        n=2.00*y;
    else if(x==5)
        n=1.50*y;
     printf("Total: R$ %.2lf\n",n);
    return 0;
}
